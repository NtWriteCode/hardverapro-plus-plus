	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: CLI",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/cli.py",
			"console": "integratedTerminal",
			"args": ["list"]
		},
		{
			"name": "Python Debugger: HAPP Module",
			"type": "debugpy",
			"request": "launch",
			"module": "hardverapro_pp"
		}
	]
}